{
  "name": "express-typescript-rest-api",
  "version": "1.0.0",
  "description": "Express Rest API with JWT Authentication",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "tsc:dev": "tsc --sourceMap true",
    "tsc:prod": "tsc",
    "build": "npm-run-all lint clean tsc:dev",
    "start": "node .",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "test": "echo \"Hello\"",
    "lint": "eslint 'src/**/*.ts'"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "shortid": "^2.2.15",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.1",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --cache"
  },
  "author": "Abdulhalim Kara",
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "typescript",
    "rest",
    "api",
    "jwt",
    "authentication",
    "boilerplate"
  ]
